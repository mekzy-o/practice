{
  "name": "practice",
  "version": "1.0.0",
  "description": "This is a practice of my andela project",
  "main": "app.js",
  "scripts": {
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "coveralls": "nyc npm test && nyc report --reporter=text-lcov | coveralls",
    "start": "babel-node ./server/app.js",
    "dev": "nodemon --exec babel-node ./server/app.js",
    "test": "set NODE_ENV='test' && npm run migrate && nyc --reporter=html --reporter=text mocha ./server/test/*.test.js --require @babel/register --exit",
    "migrate": "babel-node ./server/model/migration"
  },
  "repository": {
    "type": "git",
